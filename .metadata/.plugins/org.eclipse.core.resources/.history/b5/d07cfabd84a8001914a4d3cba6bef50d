package com.comviva.mfs.entity;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;

import lombok.EqualsAndHashCode;

@Entity
@Table(name = "MTX_DOMAINS_M")
@EqualsAndHashCode
public class DomainMaker {

	@Id
	@Column(name = "DOMAIN_CODE")
	private String domainCode;

	@Column(name = "DOMAIN_NAME")
	private String domainName;

	@Column(name = "DOMAIN_TYPE_CODE")
	private String domainTypeCode;

	@Column(name = "STATUS")
	private String status;

	@Column(name = "OWNER_CATEGORY")
	private String ownerCat;

	@Column(name = "APPLICATION_CODE")
	private String applicationCode;

	@Column(name = "NUM_OF_CATEGORIES")
	private Integer numOfCat;

	@Column(name = "CAT_PROFILE_ALLOWED")
	private String catprofileAllowed;

	@Column(name = "DOMAIN_SEQ_NO")
	private Integer domainSeqNo;

	public DomainMaker() {
		super();
	}

	public String getDomainCode() {
		return domainCode;
	}

	public void setDomainCode(String domainCode) {
		this.domainCode = domainCode;
	}
	
	public void setCatprofileAllowed(String catprofileAllowed) {
		this.catprofileAllowed = catprofileAllowed;
	}

	public String getDomainName() {
		return domainName;
	}

	public void setDomainName(String domainName) {
		this.domainName = domainName;
	}


	public void setDomainTypeCode(String domainTypeCode) {
		this.domainTypeCode = domainTypeCode;
	}

	public String getStatus() {
		return status;
	}

	public void setStatus(String status) {
		this.status = status;
	}

	public String getOwnerCat() {
		return ownerCat;
	}

	public void setOwnerCat(String ownerCat) {
		this.ownerCat = ownerCat;
	}

	public String getApplicationCode() {
		return applicationCode;
	}

	public void setApplicationCode(String applicationCode) {
		this.applicationCode = applicationCode;
	}

	public Integer getNumOfCat() {
		return numOfCat;
	}

	public void setNumOfCat(Integer numOfCat) {
		this.numOfCat = numOfCat;
	}
	

	public String getDomainTypeCode() {
		return domainTypeCode;
	}

	public String getCatprofileAllowed() {
		return catprofileAllowed;
	}

	public Integer getDomainSeqNo() {
		return domainSeqNo;
	}

	public void setDomainSeqNo(Integer domainSeqNo) {
		this.domainSeqNo = domainSeqNo;
	}

}