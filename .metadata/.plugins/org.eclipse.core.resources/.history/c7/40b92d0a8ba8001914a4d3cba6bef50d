package com.comviva.mfs.model.category;

import io.swagger.annotations.ApiModelProperty;

public class CategoryService {

	@ApiModelProperty(notes = "The Service Pymt Id", position = 1, example = "186")
	private Long servicePymtId;

	@ApiModelProperty(notes = "The Description", position = 2, example = "Bank Cash In")
	private String description;

	@ApiModelProperty(notes = "The Service Type", position = 3, example = "RBCIREQ")
	private String serviceType;

	@ApiModelProperty(notes = "The Payer Domain Code", position = 4, example = "MER")
	private String payerDomainCode;

	@ApiModelProperty(notes = "The Payee Domain Code", position = 5, example = "MERCHANT")
	private String payeeDomainCode;

	@ApiModelProperty(notes = "The Initiator Domain Code", position = 6, example = "MER")
	private String initiatorDomainCode;

	@ApiModelProperty(notes = "The Initiator Payment Instrument", position = 7, example = "BANK")
	private String initiatorPaymentInstrument;

	@ApiModelProperty(notes = "Bearer Code", position = 7, example = "J2ME")
	private String bearerCode;

	@ApiModelProperty(notes = "Is selected Y/N", position = 8, example = "Y")
	private String selected;

	public Long getServicePymtId() {
		return servicePymtId;
	}

	public void setServicePymtId(Long servicePymtId) {
		this.servicePymtId = servicePymtId;
	}

	public String getDescription() {
		return description;
	}

	public void setDescription(String description) {
		this.description = description;
	}

	public String getServiceType() {
		return serviceType;
	}

	public void setServiceType(String serviceType) {
		this.serviceType = serviceType;
	}

	public String getPayerDomainCode() {
		return payerDomainCode;
	}

	public String getPayeeDomainCode() {
		return payeeDomainCode;
	}

	public void setPayeeDomainCode(String payeeDomainCode) {
		this.payeeDomainCode = payeeDomainCode;
	}

	public String getInitiatorDomainCode() {
		return initiatorDomainCode;
	}

	public void setInitiatorDomainCode(String initiatorDomainCode) {
		this.initiatorDomainCode = initiatorDomainCode;
	}

	public String getInitiatorPaymentInstrument() {
		return initiatorPaymentInstrument;
	}

	public void setInitiatorPaymentInstrument(String initiatorPaymentInstrument) {
		this.initiatorPaymentInstrument = initiatorPaymentInstrument;
	}

	public String getBearerCode() {
		return bearerCode;
	}

	public void setBearerCode(String bearerCode) {
		this.bearerCode = bearerCode;
	}

	public String getSelected() {
		return selected;
	}
	

	public void setPayerDomainCode(String payerDomainCode) {
		this.payerDomainCode = payerDomainCode;
	}

	public void setSelected(String selected) {
		this.selected = selected;
	}

	@Override
	public String toString() {
		return "CategoryService [servicePymtId=" + servicePymtId + ", description=" + description + ", serviceType="
				+ serviceType + ", payerDomainCode=" + payerDomainCode + ", payeeDomainCode=" + payeeDomainCode
				+ ", initiatorDomainCode=" + initiatorDomainCode + ", initiatorPaymentInstrument="
				+ initiatorPaymentInstrument + ", bearerCode=" + bearerCode + ", selected=" + selected + "]";
	}

}