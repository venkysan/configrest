		this.transferControlProfileRepository = transferControlProfileRepository;
		this.transferControlProfileMakerRepository = transferControlProfileMakerRepository;
		
	}
	
	public DeleteInitiateTransferControlProfileResponse execute(String profileId, 
				DeleteInitiateTransferControlProfileRequest deleteInitiateTransferControlProfileRequest) {
		
		List<String> params = new ArrayList<>();
		try {
			this.profileId = profileId;
			this.deleteInitiateTransferControlProfileRequest = deleteInitiateTransferControlProfileRequest;
			
			
		}catch(Exception e) {
			handleException(e, params, this.getClass().getSimpleName());
		}
		
		generateReponseMessage(params);
		return createResponse();	
	}
	
