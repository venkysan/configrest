	@ApiModelProperty(notes = "Is approval required Y/N", position = 13, example = "Y")
	private String isApprovalRequired;

	@ApiModelProperty(notes = "Service Request", position = 15)
	private ServiceRole[] allServiceList;

	public String getGroupRoleCode() {
		return groupRoleCode;
	}

