import java.util.Arrays;

import com.comviva.mfs.model.BaseRequest;

import io.swagger.annotations.ApiModelProperty;

public class UpdateGroupRoleInitiateRequest extends BaseRequest {

	@ApiModelProperty(notes = "Is approval required Y/N", position = 5, example = "Y")
	private String isApprovalRequired;

	@ApiModelProperty(notes = "Service Request", position = 6)
	private ServiceRole[] allServiceList;

	public String getIsApprovalRequired() {
		return isApprovalRequired;
	}

	public void setIsApprovalRequired(String isApprovalRequired) {
		this.isApprovalRequired = isApprovalRequired;
	}

	public ServiceRole[] getAllServiceList() {
		return allServiceList;
	}

	public void setAllServiceList(ServiceRole[] allServiceList) {
		this.allServiceList = allServiceList;
	}

	@Override
	public String toString() {
		return "UpdateGroupRoleInitiateRequest [identifierType=" + identifierType + ", identifierValue="
