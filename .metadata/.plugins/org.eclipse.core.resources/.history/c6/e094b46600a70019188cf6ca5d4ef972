package com.comviva.mfs.common;

import lombok.Getter;

@Getter
public enum ServiceRoleCodes {

    ADD_DOMAIN_INIT_ROLE("ADD_INI_DOM"),
    ADD_DOMAIN_APPR_ROLE("ADD_APP_DOM"),
    UPDATE_DOMAIN_INIT_ROLE("MOD_INI_DOM"),
    UPDATE_DOMAIN_APPR_ROLE("MOD_APP_DOM"),
    DELETE_DOMAIN_INIT_ROLE("DEL_INI_DOM"),
    DELETE_DOMAIN_APPR_ROLE("DEL_APP_DOM"),
    VIEW_ALL_DOMAINS_ROLE("GET_ALL_DOM"),
	VIEW_DOMAIN_ROLE("GET_DOM"),

    ADD_CATEGORY_INIT_ROLE("ADD_CAT"),
    ADD_CATEGORY_APPR_ROLE("CAT_APPRL"),
    UPDATE_CATEGORY_INIT_ROLE("MODIFY_CAT"),
    UPDATE_CATEGORY_APPR_ROLE("UPDATE_CAT_APPR"),
    DELETE_CATEGORY_INIT_ROLE("DELETE_CAT"),
    DELETE_CATEGORY_APPR_ROLE("DELETE_CAT_APPR"),
    VIEW_ALL_CATEGORY_ROLE("GET_ALL_CAT"),
    VIEW_CATEGORY_ROLE("CAT_APPRL"),
	GET_CATEGORY_BY_CATEGORY_CODE("GET_CAT_CODE"),
	GET_CATEGORY_BY_DOMAIN_CODE("GET_CAT_DOMAIN"),
	GET_CATEGORY_RELATION_BY_CODE("GET_CAT_RELATION"),
    
    ADD_GRADE_INIT_ROLE("ADD_GRADES"),
    ADD_GRADE_APPR_ROLE("ADD_GRADES_APPR"),
    UPDATE_GRADE_INIT_ROLE("MODIFY_GRADES"),
    UPDATE_GRADE_APPR_ROLE("MODIFY_GRADES"),
    DELETE_GRADE_INIT_ROLE("DELETE_GRADES"),
    DELETE_GRADE_APPR_ROLE("DELETE_GRADES"),
    VIEW_ALL_GRADE_ROLE("VIEW_GRADES"),
    VIEW_GRADE_ROLE("VIEW_GRADES"),
    
    ADD_GROUP_ROLE_INIT_ROLE("GRP_ROL"),
    ADD_GROUP_ROLE_APPR_ROLE("GRP_ROL"),
    UPDATE_GROUP_ROLE_INIT_ROLE("GRP_ROL"),
    UPDATE_GROUP_ROLE_APPR_ROLE("GRP_ROL"),
    DELETE_GROUP_ROLE_INIT_ROLE("GRP_ROL"),
    DELETE_GROUP_ROLE_APPR_ROLE("GRP_ROL"),
    VIEW_ALL_GROUP_ROLE("GRP_ROL_VIEW"),
    VIEW_GROUP_ROLE("GRP_ROL_VIEW"),
    
    ;
	
	private String strValue;
	private int intValue;

	ServiceRoleCodes(String strValue) {
		this.strValue = strValue;
	}

	ServiceRoleCodes(int intValue) {
		this.intValue = intValue;
	}

}