	private List<Object[]> getServiceTypes() {
		List<Object[]> list = new ArrayList<>();
		Object[] objArray = new Object[2];
		objArray[0] = "VIEWBILL";
		objArray[1] = "View Bill";
		list.add(objArray);
		return list;
	}

	private List<Object[]> getServiceTypesException() {
		List<Object[]> list = new ArrayList<>();
		Object[] objArray = new Object[2];
		objArray[0] = "VIEWBILL";
		objArray[1] = 102l;
		list.add(objArray);
		return list;
	}

	private List<Object[]> getResult() {
		List<Object[]> list = new ArrayList<>();
		Object[] objArray = new Object[3];
		objArray[0] = "Cash In";
		objArray[1] = "1";
		objArray[2] = 60l;
		list.add(objArray);
		return list;

	}

	private List<Object[]> getCategoryService() {
		List<Object[]> list = new ArrayList<>();
		Object[] objArray = new Object[6];
		objArray[0] = 201l;
		objArray[1] = "IMT Stock Creation";
		objArray[2] = "CWBOTHREQ";
		objArray[3] = "MER";
		objArray[4] = "MERCHANT";
		objArray[5] = "MER";
		list.add(objArray);
		return list;
	}
	
	private List<Object[]> getlinkedWalletOrBankList() {
		List<Object[]> list = new ArrayList<>();
		Object[] objArray = new Object[2];
		objArray[0] = 40l;
		objArray[1] = "primary";
		list.add(objArray);
		return list;
	}

}
