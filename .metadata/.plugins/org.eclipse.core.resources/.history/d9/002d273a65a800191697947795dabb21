	@Test
	public void getAllServiceTypesTest() {
		when(mockedQuery.getResultList()).thenReturn(getServiceTypes());
		List<ServiceTypesResponse> listMap = supportApiCustomRepository.getAllServiceTypes();
		for (int i = 0; i < listMap.size(); i++) {
			assertThat(listMap.get(i).getServiceType(), is((getServiceTypes().get(i))[0]));
			assertThat(listMap.get(i).getServiceName(), is((getServiceTypes().get(i))[1]));
		}

	}

	@Test
	public void getAllServiceTypesExceptionTest() {
		when(mockedQuery.getResultList()).thenReturn(getServiceTypesException());
		List<ServiceTypesResponse> listMap = supportApiCustomRepository.getAllServiceTypes();
		assertTrue(listMap.isEmpty());
	}
	
	
	@Test
	public void getBankByDomainCodeDetailsCASHINTest() {
		when(mockedQuery.getResultList()).thenReturn(getlinkedWalletOrBankList());
		when(systemDataRepository.getSystemPreferenceValue(SupportApiConstants.COMMISSION_WALLET_IDENTIFIER.getStrValue())).thenReturn(String.valueOf(13));
		when(systemDataRepository.getSystemPreferenceValue(SupportApiConstants.IS_COMMISSION_WALLET_REQUIRED.getStrValue())).thenReturn("N");
		when(systemDataRepository.getSystemPreferenceValue(SupportApiConstants.IS_REMITTANCE_WALLET_REQUIRED.getStrValue())).thenReturn("TRUE");
		when(systemDataRepository.getSystemPreferenceValue(SupportApiConstants.REMITTANCE_WALLET_IDENTIFIER.getStrValue())).thenReturn(String.valueOf(14));
		when(systemDataRepository.getSystemPreferenceValue(SupportApiConstants.PROTELECOM_REMITTANCE_WALLET_IDENTIFIER.getStrValue())).thenReturn(String.valueOf(16));
		@SuppressWarnings("rawtypes")
		List<Map> listMap = supportApiCustomRepository.getWalletOrBankByDomainCodeDetails("CASHIN", 101l, "BANK", "SUBS");
		for (@SuppressWarnings("rawtypes") Map map : listMap) {
			assertThat(map.get("linkedWalletBankId"), is(((Object[]) getlinkedWalletOrBankList().get(0))[0]));
			assertThat(map.get("linkedWalletBankName"), is(((Object[]) getlinkedWalletOrBankList().get(0))[1]));
		}
		
	}
	
	
