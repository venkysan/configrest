	private static AddGroupRoleInitiateRequest getAddGroupRoleInitiateRequest() {
		AddGroupRoleInitiateRequest addGroupRoleInitiateRequest = new AddGroupRoleInitiateRequest();
		ServiceRole agroupRoleServiceRequest = new ServiceRole();
		addGroupRoleInitiateRequest.setBearerCode(TestBaseConstants.BEARER_CODE_VALUE.getValue());
		addGroupRoleInitiateRequest.setExternalRefId(GroupRoleTestConstants.EXTERNAL_REF_ID.getValue());
		addGroupRoleInitiateRequest.setIdentifierType(TestBaseConstants.IDENTIFIER_TYPE_VALUE.getValue());
		addGroupRoleInitiateRequest.setIdentifierValue(TestBaseConstants.IDENTIFIER_VALUE_NETADMIN_VALUE.getValue());
		addGroupRoleInitiateRequest.setGroupRoleCode(GroupRoleTestConstants.GROUP_ROLE_CODE.getValue());
		addGroupRoleInitiateRequest.setGroupRoleName(GroupRoleTestConstants.GROUP_ROLE_NAME.getValue());
		addGroupRoleInitiateRequest.setCategoryCode(GroupRoleTestConstants.CATEGORY_CODE.getValue());
		addGroupRoleInitiateRequest.setGradeCode(GroupRoleTestConstants.GRADE_CODE.getValue());
		addGroupRoleInitiateRequest
				.setGroupRoleType(Integer.parseInt(GroupRoleTestConstants.GROUP_ROLE_TYPE.getValue()));
		addGroupRoleInitiateRequest.setProviderId(Long.valueOf(GroupRoleTestConstants.PROVIDER_ID.getValue()));
		addGroupRoleInitiateRequest.setPaymentType(GroupRoleTestConstants.PAYMENT_TYPE.getValue());
		addGroupRoleInitiateRequest.setPaymentTypeId(Long.valueOf(GroupRoleTestConstants.PAYMENT_TYPE_ID.getValue()));
		addGroupRoleInitiateRequest.setLinkedbanks(GroupRoleTestConstants.LINKEDBANKS.getValue());
		addGroupRoleInitiateRequest.setIsApprovalRequired(GroupRoleTestConstants.IS_APPROVAL_REQUIRED_YES.getValue());
		agroupRoleServiceRequest.setServiceName(GroupRoleTestConstants.SERVICE_NAME.getValue());
		agroupRoleServiceRequest.setServiceType(GroupRoleTestConstants.SERVICE_TYPE.getValue());
		agroupRoleServiceRequest.setServiceName(GroupRoleTestConstants.SELECTED.getValue());
		ServiceRole groupRoleServiceRequest[] = new ServiceRole[1];
		ServiceRole serviceRole = new ServiceRole();
		serviceRole.setServiceName("C2C Sender");
		serviceRole.setServiceType("50010");
		serviceRole.setServiceStatus("Y");
		groupRoleServiceRequest[0] = serviceRole;
		addGroupRoleInitiateRequest.setAllServiceList(groupRoleServiceRequest);
		return addGroupRoleInitiateRequest;

	}

	public static List<SysGroupRoleChecker> getGroupRoles() {
		List<SysGroupRoleChecker> list = new ArrayList<SysGroupRoleChecker>();
		list.add(getGroupRolesChecker());
		return list;
	}
	
	public static SysGroupRoleChecker getGroupRolesChecker() {
		SysGroupRoleChecker sysGroupRoleChecker = new SysGroupRoleChecker();
		sysGroupRoleChecker.setCategoryCode(GroupRoleTestConstants.CATEGORY_CODE.getValue());
		sysGroupRoleChecker.setGroupRoleCode(GroupRoleTestConstants.GROUP_ROLE_CODE.getValue());
		sysGroupRoleChecker.setStatus(GroupRoleTestConstants.STATUS.getValue());
		sysGroupRoleChecker.setGroupRoleName(GroupRoleTestConstants.GROUP_ROLE_NAME.getValue());
		sysGroupRoleChecker.setGroupRoleType(GroupRoleTestConstants.WEB_GROUP_ROLE_TYPE.getValue());
		sysGroupRoleChecker.setProviderId(10L);
		sysGroupRoleChecker.setBankId(GroupRoleTestConstants.BANK_ID.getValue());
		sysGroupRoleChecker.setPaymentTypeId(12L);
		sysGroupRoleChecker.setDeFault(GroupRoleTestConstants.IS_DEFAULT.getValue());
		sysGroupRoleChecker.setGradeCode(GroupRoleTestConstants.GRADE_CODE.getValue());
		return sysGroupRoleChecker;
	}

	public static SysGroupRoleMaker getGroupRolesMaker() {
		SysGroupRoleMaker sysGroupRoleMaker = new SysGroupRoleMaker();
		sysGroupRoleMaker.setCategoryCode(GroupRoleTestConstants.CATEGORY_CODE.getValue());
		sysGroupRoleMaker.setGroupRoleCode(GroupRoleTestConstants.GROUP_ROLE_CODE.getValue());
		sysGroupRoleMaker.setStatus(GroupRoleTestConstants.STATUS.getValue());
		sysGroupRoleMaker.setGroupRoleName(GroupRoleTestConstants.GROUP_ROLE_NAME.getValue());
		sysGroupRoleMaker.setGroupRoleType(GroupRoleTestConstants.GROUP_ROLE_TYPE.getValue());
		sysGroupRoleMaker.setProviderId(10L);
		sysGroupRoleMaker.setBankId(GroupRoleTestConstants.BANK_ID.getValue());
		sysGroupRoleMaker.setPaymentTypeId(12L);
		sysGroupRoleMaker.setDeFault(GroupRoleTestConstants.IS_DEFAULT.getValue());
		sysGroupRoleMaker.setGradeCode(GroupRoleTestConstants.GRADE_CODE.getValue());
		return sysGroupRoleMaker;
	}

	public static List<SysGroupRoleChecker> getGroupRolesEmpty() {
		List<SysGroupRoleChecker> list = new ArrayList<SysGroupRoleChecker>();
		list.add(null);
		return list;
	}

	public static SysGroupRoleChecker getGroupRolesCheckerEmpty() {
		SysGroupRoleChecker sysGroupRoleCheckerEmpty = new SysGroupRoleChecker();
		return sysGroupRoleCheckerEmpty;
	}

	private static String getGroupRolesBasicUrl(String prefixUrl, String suffixUrl) {
		StringBuilder sb = new StringBuilder();
		sb.append(TestBaseConstants.BASE_URL.getValue());
		sb.append(prefixUrl);
		sb.append(TestBaseConstants.QUESTION_MARK_SYMBOL.getValue());
		sb.append(TestBaseConstants.IDENTIFIER_TYPE.getValue());
		sb.append(TestBaseConstants.EQUAL_SYMBOL.getValue());
		sb.append(TestBaseConstants.IDENTIFIER_TYPE_VALUE.getValue());
		sb.append(TestBaseConstants.AND_SYMBOL.getValue());
		sb.append(TestBaseConstants.IDENTIFIER_VALUE.getValue());
		sb.append(TestBaseConstants.EQUAL_SYMBOL.getValue());
		sb.append(TestBaseConstants.IDENTIFIER_VALUE_NETADMIN_VALUE.getValue());
		sb.append(TestBaseConstants.AND_SYMBOL.getValue());
		sb.append(TestBaseConstants.BEARER_CODE.getValue());
		sb.append(TestBaseConstants.EQUAL_SYMBOL.getValue());
		sb.append(TestBaseConstants.BEARER_CODE_VALUE.getValue());
		sb.append(suffixUrl);
		return sb.toString();
	}

	public static String updateGroupRoleInitialRequest() {
		return CommonUtilTest.convertObjectToJson(getUpdateGroupRoleInitialRequest());
	}

	public static UpdateGroupRoleInitiateRequest getUpdateGroupRoleInitialRequest() {
		UpdateGroupRoleInitiateRequest updateInitiateGroupRole = new UpdateGroupRoleInitiateRequest();
		updateInitiateGroupRole.setBearerCode(TestBaseConstants.BEARER_CODE_VALUE.getValue());
		updateInitiateGroupRole.setExternalRefId(CategoryTestConstants.EXTERNAL_REF_ID.getValue());
		updateInitiateGroupRole.setIdentifierType(TestBaseConstants.IDENTIFIER_TYPE_VALUE.getValue());
		updateInitiateGroupRole.setIdentifierValue(TestBaseConstants.IDENTIFIER_VALUE_NETADMIN_VALUE.getValue());
		updateInitiateGroupRole.setIsApprovalRequired(CategoryTestConstants.IS_APPROVAL_REQUIRED.getValue());
		ServiceRole groupRoleServiceRequest[] = new ServiceRole[1];
		ServiceRole serviceRole = new ServiceRole();
		serviceRole.setServiceName("C2C Sender");
		serviceRole.setServiceType("50010");
		serviceRole.setServiceStatus("Y");
		groupRoleServiceRequest[0] = serviceRole;
		updateInitiateGroupRole.setAllServiceList(groupRoleServiceRequest);
		return updateInitiateGroupRole;
	}

