	private static AddGroupRoleApproveRequest getAddGroupRoleApproveRequest() {
		AddGroupRoleApproveRequest addApproveGroupRole = new AddGroupRoleApproveRequest();
		addApproveGroupRole.setBearerCode(TestBaseConstants.BEARER_CODE_VALUE.getValue());
		addApproveGroupRole.setExternalRefId(CategoryTestConstants.EXTERNAL_REF_ID.getValue());
		addApproveGroupRole.setIdentifierType(TestBaseConstants.IDENTIFIER_TYPE_VALUE.getValue());
		addApproveGroupRole.setIdentifierValue(TestBaseConstants.IDENTIFIER_VALUE_NETADMIN_VALUE.getValue());
		return addApproveGroupRole;
	}

