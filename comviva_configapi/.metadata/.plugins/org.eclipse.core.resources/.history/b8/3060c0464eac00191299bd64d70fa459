package com.comviva.mfs.constant.test;

public enum TestBaseConstants {
	
	IDENTIFIER_TYPE("identifierType"),
	IDENTIFIER_VALUE("identifierValue"),
	BEARER_CODE("bearerCode"),
	IDENTIFIER_TYPE_VALUE("loginId"),
	IDENTIFIER_VALUE_NETADMIN_VALUE("netadmin"),
	IDENTIFIER_VALUE_CHANNELADMIN_VALUE("chadmin1"),
	IDENTIFIER_VALUE_SUPER_ADMIN_VALUE("superadminm"),
	BEARER_CODE_VALUE("WEB"),
	INITIATEDTYPE("initiatedType"), 
	NO("N"),
	YES("Y"),
	EQUAL_SYMBOL("="),
	AND_SYMBOL("&"),
	QUESTION_MARK_SYMBOL("?"),
	BASE_URL("/v1/"),
	MESSAGE("message"),
	STATUS("status"),
	MESSAGE_CODE("messageCode"),
	SUCCESS_VAL("Success"),
	VALIDATION_ERROR_VAL("400"),
	SUCCESS_STATUS("200"),
	NULL(null),
	VALUE_NULL(""),
	// Test result Constants  do not change
	RESULT_STATUS_JSON_FIELD("$.status"),
	
	;
	private String baseValue;
	
	private TestBaseConstants(String value) {
		this.baseValue = value;
	}
	
	public String getValue() {
		return this.baseValue;
	}
}
