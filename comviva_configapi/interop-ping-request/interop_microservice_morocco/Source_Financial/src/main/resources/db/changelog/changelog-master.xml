<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <changeSet id="2" author="comviva">
        <createTable tableName="channel_user_details">
            <column name="user_id" type="VARCHAR(50)" remarks="channel user id">
                <constraints primaryKey="true" primaryKeyName="pk_userid" />
            </column>
            <column name="msisdn" type="VARCHAR(10)" remarks="Telephone number">
            </column>
            <column name="channel_user_code" type="VARCHAR(100)" remarks="channel user code">
                <constraints nullable="false" />
            </column>
            <column name="description" type="VARCHAR(255)">
                <constraints nullable="false" />
            </column>
            <column name="type" type="VARCHAR(50)" remarks="channel user type">
                <constraints nullable="false" />
            </column>
            <column name="optional" type="VARCHAR(255)">
                <constraints nullable="true" />
            </column>
            <column name="created_date" type="DATETIME">
                <constraints nullable="false" />
            </column>
            <column name="updated_date" type="DATETIME">
                <constraints nullable="true" />
            </column>
            <column name="country_id" type="VARCHAR(10)">
                <constraints nullable="false" />
            </column>
        </createTable>

        <createTable tableName="interop_transactions">
            <column name="INTEROP_TXN_ID" type="VARCHAR(255)"
                remarks="it acts as a transaction id">
                <constraints nullable="false" primaryKeyName="pk_id_txn" primaryKey="true" />
            </column>
            <column name="TRANSACTION_TYPE" type="VARCHAR(50)" remarks="Type of Transaction">
                <constraints nullable="false" />
            </column>
            <column name="AMOUNT" type="DECIMAL(15,2)" remarks="transaction amount">
                <constraints nullable="false" />
            </column>
            <column name="CURRENCY" type="VARCHAR(50)">
                <constraints nullable="false" />
            </column>
            <column name="REQUEST_SOURCE" type="VARCHAR(255)">
                <constraints nullable="true" />
            </column>
            <column name="EXT_ORG_REF_ID" type="VARCHAR(255)">
                <constraints nullable="true" />
            </column>
            <column name="TXN_STATUS" type="VARCHAR(5)">
                <constraints nullable="false" />
            </column>
            <column name="PAYER_CHANNEL_USER_CODE" type="VARCHAR(100)">
                <constraints nullable="true" />
            </column>
            <column name="PAYER_MSISDN" type="VARCHAR(15)">
                <constraints nullable="false" />
            </column>
            <column name="PAYER_PROVIDER" type="VARCHAR(15)">
                <constraints nullable="false" />
            </column>
            <column name="PAYER_PAY_ID" type="VARCHAR(10)">
                <constraints nullable="false" />
            </column>
            <column name="IS_PAYER_TECHNICAL" type="TINYINT(1)">
                <constraints nullable="true" />
            </column>
            <column name="PAYEE_CHANNEL_USER_CODE" type="VARCHAR(100)">
                <constraints nullable="true" />
            </column>
            <column name="PAYEE_MSISDN" type="VARCHAR(15)">
                <constraints nullable="false" />
            </column>
            <column name="PAYEE_PROVIDER" type="VARCHAR(15)">
                <constraints nullable="false" />
            </column>
            <column name="PAYEE_PAY_ID" type="VARCHAR(10)">
                <constraints nullable="false" />
            </column>
            <column name="IS_PAYEE_TECHNICAL" type="TINYINT(1)">
                <constraints nullable="true" />
            </column>
            <column name="created_date" type="DATETIME">
                <constraints nullable="false" />
            </column>
            <column name="updated_date" type="DATETIME">
                <constraints nullable="true" />
            </column>
            <column name="HPS_REFERENCE_NUMBER" type="VARCHAR(50)"
                remarks="Reference number of operation generated by Mobile Switch">
                <constraints unique="true" uniqueConstraintName="unq_hpsrefno"></constraints>
            </column>
        </createTable>

        <createIndex indexName="interop_transactions_index"
            tableName="interop_transactions" unique="true">
            <column name="INTEROP_TXN_ID" />
            <column name="created_date" />
        </createIndex>

        <createSequence sequenceName="id_seq" cycle="false"
            incrementBy="1" minValue="1" maxValue="999999" startValue="1" />

        <createSequence sequenceName="stan_seqid" cycle="true"
            incrementBy="1" minValue="1" maxValue="999999" startValue="1" />

        <createSequence sequenceName="reference_seqid" cycle="true"
            incrementBy="1" minValue="1" maxValue="999999999999" startValue="1" />

        <createTable tableName="interop_transactions_details">
            <column name="id" type="bigint" remarks="primary id">
                <constraints nullable="false" primaryKeyName="pk_id_txn_details" primaryKey="true" />
            </column>
            <column name="INTEROP_TXN_ID" type="VARCHAR(255)"
                remarks="it acts as a transaction id">
                <constraints nullable="false" />
            </column>
            <column name="PAYER_PAY_ID" type="VARCHAR(10)">
                <constraints nullable="false" />
            </column>
            <column name="PAYEE_PAY_ID" type="VARCHAR(10)">
                <constraints nullable="false" />
            </column>
            <column name="THIRD_PARTY_REF_ID" type="VARCHAR(100)">
                <constraints nullable="true" />
            </column>
            <column name="THIRD_PARTY_TXN_TYPE" type="VARCHAR(50)"
                remarks="Third party transaction type">
                <constraints nullable="false" />
            </column>
            <column name="THIRD_PARTY_REQ_TYPE" type="VARCHAR(50)"
                remarks="Third party request type">
                <constraints nullable="false" />
            </column>
            <column name="THIRD_PARTY_PAYER" type="VARCHAR(50)" remarks="Third party payer">
                <constraints nullable="true" />
            </column>
            <column name="THIRD_PARTY_PAYEE" type="VARCHAR(50)" remarks="Third party payee">
                <constraints nullable="true" />
            </column>
            <column name="AMOUNT" type="DECIMAL(15,2)" remarks="transaction amount">
                <constraints nullable="false" />
            </column>
            <column name="CURRENCY" type="VARCHAR(50)" remarks="transaction currency">
                <constraints nullable="false" />
            </column>
            <column name="TXN_STATUS" type="VARCHAR(5)">
                <constraints nullable="true" />
            </column>
            <column name="THIRD_PARTY_RESPONSE_CODE" type="VARCHAR(50)">
                <constraints nullable="true" />
            </column>
            <column name="STAN" type="VARCHAR(15)">
                <constraints nullable="true" />
            </column>
            <column name="AUTH_CODE" type="VARCHAR(50)">
                <constraints nullable="true" />
            </column>
            <column name="THIRD_PARTY_MAPPING_CODE" type="VARCHAR(255)">
                <constraints nullable="true" />
            </column>
            <column name="THIRD_PARTY_RESPONSE_MESSAGE" type="VARCHAR(400)">
                <constraints nullable="true" />
            </column>
            <column name="TXN_MODE" type="VARCHAR(255)">
                <constraints nullable="true" />
            </column>
            <column name="REMARKS" type="VARCHAR(255)">
                <constraints nullable="true" />
            </column>
            <column name="created_date" type="DATETIME">
                <constraints nullable="false" />
            </column>
            <column name="updated_date" type="DATETIME">
                <constraints nullable="true" />
            </column>
        </createTable>

        <createTable tableName="languages">
            <column name="language" type="VARCHAR(20)" remarks="Language">
                <constraints nullable="false" />
            </column>
            <column name="lang_code" type="VARCHAR(2)" remarks="ISO language code">
                <constraints primaryKey="true" primaryKeyName="pk_lang_code" />
            </column>
        </createTable>

        <createTable tableName="statuscodes">
            <column name="statuscode" type="VARCHAR(5)" remarks="Error/Status code">
                <constraints nullable="false" />
            </column>
            <column name="entity" type="VARCHAR(50)"
                remarks="defines identity of third party responsible for Error/Status code">
                <constraints nullable="false" />
            </column>
            <column name="lang_code" type="VARCHAR(2)" remarks="ISO language code">
                <constraints nullable="false" references="languages(lang_code)"
                    foreignKeyName="fk_lang_code" />
            </column>
            <column name="mapping_code" type="VARCHAR(250)"
                remarks="mapping code for the specific scenario">
                <constraints nullable="true" />
            </column>
            <column name="description" type="VARCHAR(255)" remarks="description for the code">
                <constraints nullable="false" />
            </column>
        </createTable>

        <addUniqueConstraint columnNames="statuscode,entity,lang_code"
            constraintName="pk_statuscodes" tableName="statuscodes" />

        <createTable tableName="sms_templates">
            <column name="id" type="VARCHAR(5)" remarks="template id">
                <constraints nullable="false" />
            </column>
            <column name="notification_code" type="VARCHAR(100)" remarks="notification code">
                <constraints nullable="true" />
            </column>
            <column name="language_code" type="VARCHAR(2)" remarks="language code">
                <constraints nullable="false" />
            </column>
            <column name="description" type="VARCHAR(255)" remarks="sms description">
                <constraints nullable="false" />
            </column>
        </createTable>

        <createTable tableName="sms_delivery">
            <column name="sms_id" type="VARCHAR(32)" remarks="SMS Id">
                <constraints nullable="false" />
            </column>
            <column name="msisdn" type="VARCHAR(15)" remarks="MSISDN">
                <constraints nullable="false" />
            </column>
            <column name="message" type="VARCHAR(400)" remarks="SMS Message">
                <constraints nullable="false" />
            </column>
            <column name="language_code" type="VARCHAR(2)" remarks="language code">
                <constraints nullable="false" />
            </column>
            <column name="status" type="VARCHAR(2)"
                remarks="Delevered = Y , Not Delevered = N ">
                <constraints nullable="false"></constraints>
            </column>
            <column name="delivered_on" type="TIMESTAMP" remarks="deleivery date">
                <constraints nullable="true" />
            </column>
            <column name="retry_count" type="INTEGER" remarks="retry count"
                defaultValue="0" />
            <column name="service_type" type="VARCHAR(32)" remarks="Service type">
                <constraints nullable="true" />
            </column>
            <column name="txn_id" type="VARCHAR(20)" remarks="transaction id">
                <constraints nullable="true" />
            </column>
            <column name="txn_status" type="VARCHAR(15)" remarks="Transaction Status">
                <constraints nullable="true" />
            </column>
            <column name="transfer_date" type="TIMESTAMP" remarks="transfer date">
                <constraints nullable="true" />
            </column>
            <column name="sender" type="VARCHAR(15)" remarks="sender">
                <constraints nullable="true" />
            </column>
            <column name="reciever" type="VARCHAR(15)" remarks="reciever">
                <constraints nullable="true" />
            </column>
            <column name="created_on" type="TIMESTAMP" remarks="creation date">
                <constraints nullable="true" />
            </column>
            <column name="node_name" type="VARCHAR(15)"
                remarks="name of the node where the application is running">
                <constraints nullable="true" />
            </column>
        </createTable>

        <rollback>
            <dropTable tableName="channel_user_details" />
            <dropTable tableName="interop_transactions" />
            <dropTable tableName="interop_transactions_details" />
            <dropTable tableName="sms_templates" />
            <dropTable tableName="sms_delivery" />
            <dropTable tableName="statuscodes" />
        </rollback>
    </changeSet>

